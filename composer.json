{
    "name": "brianhenryie/bh-phpunit-failed-tests-action",
    "description": "Finds failed tests from recent CI runs and re-runs them first, giving faster feedback on whether previous failures are fixed.",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "BrianHenryIE"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "8.0"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
