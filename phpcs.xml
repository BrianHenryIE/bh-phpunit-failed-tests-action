<?xml version="1.0"?>
<ruleset name="PHPCompatibility">
    <description>Check PHP 7.4 compatibility.</description>

    <rule ref="PHPCompatibility"/>

    <config name="testVersion" value="7.4-"/>

    <file>src</file>
    <file>bin</file>
    <file>tests</file>

    <arg name="extensions" value="php"/>
    <arg value="sp"/>
</ruleset>
